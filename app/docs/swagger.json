    "paths": {
        "/login/chkLogin": {
            "get": {
                "description": "check user is logined",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "Login"
                ],
                "summary": "user's login check",
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "$ref": "#/definitions/define.BsmgMemberResponse"
                        }
                    }
                }
            }
        },
        "/login/login": {
            "post": {
                "description": "Login must always precede.",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "Login"
                ],
                "summary": "Login",
                "parameters": [
                    {
                        "description": "login Info",
                        "name": "Data",
                        "in": "body",
                        "required": true,
                        "schema": {
                            "$ref": "#/definitions/define.BsmgMemberLoginRequest"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "$ref": "#/definitions/define.BsmgMemberResponse"
                        }
                    }
                }
            }
        },
        "/reportList": {
            "get": {
                "description": "Get Daily Report List des",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "Report"
                ],
                "summary": "get report List summary",
                "parameters": [
                    {
                        "enum": [
                            "All",
                            "Title",
                            "Content",
                            "Reporter"
                        ],
                        "type": "string",
                        "description": "Search Condition enums",
                        "name": "search_combo",
                        "in": "query",
                        "required": true
                    },
                    {
                        "type": "string",
                        "description": "Search Input",
                        "name": "search_input",
                        "in": "query"
                    },
                    {
                        "type": "integer",
                        "description": "offset",
                        "name": "offset",
                        "in": "query",
                        "required": true
                    },
                    {
                        "type": "integer",
                        "description": "limit",
                        "name": "limit",
                        "in": "query",
                        "required": true
                    }
                ],
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "$ref": "#/definitions/define.BsmgReportListResponse"
                        }
                    }
                }
            }
        }
    },
